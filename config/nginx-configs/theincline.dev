server {

    # Uncomment the following line for domain mapping
    # listen 80 default_server;

    server_name theincline.dev   *.theincline.dev ;

    # Uncomment the following line for domain mapping
    server_name_in_redirect off;

    access_log /var/log/nginx/theincline.dev.access.log rt_cache;
    error_log /var/log/nginx/theincline.dev.error.log;


    root /var/www/spiritedmedia.dev/htdocs;



    index index.php index.html index.htm;


    include common/php7.conf;

    include common/wpcommon-php7.conf;
    include common/locations-php7.conf;
    include /var/www/spiritedmedia.dev/conf/nginx/*.conf;
}
